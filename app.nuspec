<?xml version="1.0" encoding="UTF-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>Com.Latipium.DevTools</id>
        <version>{{ version }}</version>
        <title>Latipium Development Tools</title>
        <authors>Zach Deibert</authors>
        <description>Tools needed for developing Latipium mods</description>
        <releaseNotes></releaseNotes>
        <summary>Tools needed for developing Latipium mods</summary>
        <language>en-US</language>
        <projectUrl>https://latipium.com/</projectUrl>
        <iconUrl>https://latipium.com/favicon.ico</iconUrl>
        <licenseUrl>https://latipium.com/license</licenseUrl>
        <copyright>Copyright (c) 2016 Zach Deibert</copyright>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <dependencies>
            <dependency id="log4net" version="2.0.5" />
            <dependency id="CommandLineParser" version="1.9.71" />
            <dependency id="GitSharp" version="0.3" />
            <dependency id="NuGet.Core" version="2.12.0" />
            <dependency id="Newtonsoft.Json" version="9.0.1" />
            <dependency id="SharpZipLib" version="0.86.0" />
        </dependencies>
        <frameworkAssemblies>
            <frameworkAssembly assemblyName="System" />
            <frameworkAssembly assemblyName="System.ComponentModel.DataAnnotations" />
            <frameworkAssembly assemblyName="System.Net.Http" />
            <frameworkAssembly assemblyName="System.Xml" />
        </frameworkAssemblies>
        <tags>Latipium Development Non-Mod</tags>
    </metadata>
    <files>
        <file src="bin/{{ configuration }}/Com.Latipium.DevTools.exe" target="bin" />
        <file src="bin/{{ configuration }}/logging.xml" target="etc" />
        <file src="bin/{{ configuration }}/logging-verbose.xml" target="etc" />
    </files>
</package>
